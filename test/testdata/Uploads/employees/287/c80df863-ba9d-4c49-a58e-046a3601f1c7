<script>
$(window).scroll(function() {
    scr_top = $(window).scrollTop();
    top_init  = 250;
    top_float = 60;
    gap = top_init-scr_top;

    if(gap>=top_float) {
        real_top = gap;
        $("#fixme").css('top',real_top+'px');
		$("#fixme_r").css('top',real_top+'px');
    }
    else {
        real_top = top_float;
        $("#fixme").css('top',real_top+'px');
		$("#fixme_r").css('top',real_top+'px');
    }  
});
</script>
